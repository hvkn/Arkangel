<!DOCTYPE html>
<html lang="en">
<% include css/webCss %>

    <body>
        <div class="content">
            <% include common/commonNavbar %>
                <form method="post">
                    <div class="container">
                        <nav class="mt-5">
                            <div class="nav nav-tabs" id="nav-tab" role="tablist">
                                <a class="nav-item nav-link active" id="nav-image-tab" data-toggle="tab" href="#nav-image" role="tab" aria-controls="nav-image"
                                    aria-selected="false">Settings</a>
                                <a class="nav-item nav-link" id="nav-log-tab" data-toggle="tab" href="#nav-log" role="tab" aria-controls="nav-log" aria-selected="true">All</a>
                                <a class="nav-item nav-link" id="nav-history-tab" data-toggle="tab" href="#nav-history" role="tab" aria-controls="nav-history" aria-selected="true">History</a>
                            </div>
                        </nav>
                        <div class="col-md-12">
                            <div class="tab-content" id="nav-tabContent">
                                <div class="tab-pane fade show active" id="nav-image" role="tabpanel" aria-labelledby="nav-image-tab">
                                    <div class="offset-md-3">
                                        <div class="col-md-10">
                                            <table class="mt-5">
                                                <tbody>
                                                    <tr>
                                                        <td>
                                                            <i class="fab fa-chrome icon-settings"></i>
                                                        </td>
                                                        <td class="pt-single-text">
                                                            Enable get History from Browsers
                                                        </td>
                                                        <td class="pr-2 ml-5" style="float: right">
                                                            <label class="switch">
                                                                <input type="checkbox" name="enable" id="enable">
                                                                <span class="slider"></span>
                                                            </label>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <i class="fas fa-bookmark icon-settings"></i>
                                                        </td>
                                                        <td class="pt-single-text">
                                                            Get bookmark
                                                        </td>
                                                        <td class="pr-2 ml-5" style="float: right">
                                                            <label class="switch">
                                                                <input type="checkbox" name="bookmark" id="bookmark">
                                                                <span class="slider"></span>
                                                            </label>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <i class="fas fa-key icon-settings"></i>
                                                        </td>
                                                        <td class="pt-single-text">
                                                            Get saved password
                                                        </td>
                                                        <td class="pr-2 ml-5" style="float: right">
                                                            <label class="switch">
                                                                <input type="checkbox" name="password" id="password">
                                                                <span class="slider"></span>
                                                            </label>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td colspan="3">
                                                            <div class="offset-md-4 mt-4">
                                                                <button type="submit" formaction="/web-usage" class="btn btn-success btn-lg">OK</button>
                                                                <button type="button" class="btn btn-danger btn-lg">
                                                                    <a class="cancel" href="/web-usage">Cancel</a>
                                                                </button>
                                                            </div>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                                <div class="tab-pane fade" id="nav-log" role="tabpanel" aria-labelledby="nav-log-tab">
                                    <div id="timeline" class="mt-5"></div>
                                </div>
                                <div class="tab-pane fade" id="nav-history" role="tabpanel" aria-labelledby="nav-history-tab">
                                    <div id="history" style="min-width: 300px; height: 400px; margin: 0 auto" class="mt-5"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
        </div>
        <% include common/commonFooter %>
            <% include script/webScript %>
    </body>

</html>